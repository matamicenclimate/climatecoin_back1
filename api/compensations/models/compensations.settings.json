{
  "kind": "collectionType",
  "collectionName": "compensations",
  "info": {
    "name": "Compensations",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nfts": {
      "collection": "nfts"
    },
    "txn_id": {
      "type": "string"
    },
    "amount": {
      "type": "biginteger"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "created",
        "pending_notification",
        "pending_certificates",
        "received_certificates",
        "minted"
      ],
      "default": "created"
    },
    "consolidation_certificate": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "registry_certificates": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "compensation_nft": {
      "model": "nfts"
    }
  }
}
